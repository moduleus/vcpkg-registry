{
  "$schema": "https://raw.githubusercontent.com/microsoft/vcpkg-tool/main/docs/vcpkg.schema.json",
  "name": "uac",
  "version": "1.0.2",
  "port-version": 0,
  "description": "Ultrasound Acquisition Configuration format",
  "homepage": "https://www.moduleus.com/",
  "license": "LGPL-3.0-or-later",
  "dependencies": [
    {
      "name": "vcpkg-cmake",
      "host": true
    },
    {
      "name": "vcpkg-cmake-config",
      "host": true
    }
  ],
  "features": {
    "hdf5": {
      "description": "Enable read / write of urx files",
      "dependencies": [
        {
          "name": "hdf5",
          "default-features": false,
          "features": [
            "cpp"
          ]
        }
      ]
    },
    "matlab": {
      "description": "Enable Matlab. Need hdf5 v1.8"
    },
    "python": {
      "description": "Enable python binding",
      "dependencies": [
        "pybind11"
      ]
    },
    "python-wheel": {
      "description": "Build python wheel package",
      "dependencies": [
        {
          "name": "uac",
          "default-features": false,
          "features": [
            "python"
          ]
        }
      ]
    },
    "urx-python-wheel": {
      "description": "Need urx python wheel package",
      "dependencies": [
        {
          "name": "urx",
          "default-features": false,
          "features": [
            "python",
            "python-wheel"
          ]
        }
      ]
    },
    "tests": {
      "description": "Enable testing",
      "dependencies": [
        "catch2"
      ]
    },
    "urx": {
      "description": "Use URX[] from VCPKG",
      "dependencies": [
        {
          "name": "urx",
          "default-features": false
        }
      ]
    },
    "urx-hdf5": {
      "description": "Use URX[hdf5] from VCPKG",
      "dependencies": [
        {
          "name": "urx",
          "default-features": false,
          "features": [
            "hdf5"
          ]
        }
      ]
    },
    "urx-matlab": {
      "description": "Use URX[matlab] from VCPKG",
      "dependencies": [
        {
          "name": "urx",
          "features": [
            "matlab"
          ]
        }
      ]
    },
    "urx-python": {
      "description": "Use URX[python] from VCPKG",
      "dependencies": [
        {
          "name": "urx",
          "features": [
            "python"
          ]
        }
      ]
    },
    "urx-tests": {
      "description": "Use URX[tests] from VCPKG",
      "dependencies": [
        {
          "name": "urx",
          "features": [
            "tests"
          ]
        }
      ]
    }
  },
  "overrides": [
    {
      "name": "hdf5",
      "version": "1.8.23"
    },
    {
      "name": "urx",
      "version": "1.0.1"
    }
  ]
}