{
  "$schema": "https://raw.githubusercontent.com/microsoft/vcpkg-tool/main/docs/vcpkg.schema.json",
  "name": "urx",
  "version": "1.1.0",
  "description": "Ultrasound Raw eXchange format",
  "homepage": "https://www.moduleus.com/",
  "license": "MIT",
  "dependencies": [
    {
      "name": "vcpkg-cmake",
      "host": true
    },
    {
      "name": "vcpkg-cmake-config",
      "host": true
    }
  ],
  "features": {
    "hdf5": {
      "description": "Enable read / write of hdf files",
      "dependencies": [
        {
          "name": "hdf5",
          "default-features": false,
          "features": [
            "cpp"
          ]
        }
      ]
    },
    "matlab": {
      "description": "Enable Matlab. Need hdf5 v1.8"
    },
    "python": {
      "description": "Enable python binding",
      "dependencies": [
        "pybind11"
      ]
    },
    "python-wheel": {
      "description": "Build python wheel package",
      "dependencies": [
        {
          "name": "urx",
          "default-features": false,
          "features": [
            "python"
          ]
        }
      ]
    },
    "tests": {
      "description": "Enable testing",
      "dependencies": [
        "catch2"
      ]
    }
  },
  "overrides": [
    {
      "name": "hdf5",
      "version": "1.8.23"
    }
  ]
}